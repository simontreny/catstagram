{
  "name": "Catstagram",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "micro-signals": "^2.1.0",
    "react": "16.8.3",
    "react-native": "0.62.3",
    "react-native-gesture-handler": "^1.3.0",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/runtime": "7.5.5",
    "@types/detox": "^12.8.3",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
    "@types/react-navigation": "^3.0.7",
    "babel-jest": "24.8.0",
    "detox": "^13.2.0",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "0.55.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "16.8.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Catstagram/Build/Products/Debug-iphonesimulator/Catstagram.app",
        "build": "xcodebuild -project ios/Catstagram.xcodeproj -scheme Catstagram -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build/Catstagram",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Catstagram/Build/Products/Release-iphonesimulator/Catstagram.app",
        "build": "xcodebuild -project ios/Catstagram.xcodeproj -scheme Catstagram -configuration Release -sdk iphonesimulator -derivedDataPath ios/build/Catstagram",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5_API_27"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5_API_27"
      }
    },
    "test-runner": "jest"
  }
}
