{
	"name": "Catstagram",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint ."
	},
	"dependencies": {
		"@react-native-community/async-storage": "^1.5.1",
		"micro-signals": "^2.1.0",
		"react": "16.8.6",
		"react-native": "0.60.4",
		"react-native-gesture-handler": "^1.3.0",
		"react-navigation": "^3.11.1"
	},
	"devDependencies": {
		"@babel/core": "7.5.5",
		"@babel/runtime": "7.5.5",
		"@react-native-community/eslint-config": "0.0.3",
		"@types/detox": "^12.8.2",
		"@types/jest": "^24.0.15",
		"@types/react": "^16.8.23",
		"@types/react-native": "^0.60.2",
		"@types/react-navigation": "^3.0.7",
		"babel-jest": "24.8.0",
		"detox": "^13.2.0",
		"eslint": "6.1.0",
		"jest": "24.8.0",
		"metro-react-native-babel-preset": "0.54.1",
		"prettier": "^1.18.2",
		"react-test-renderer": "16.8.6",
		"ts-jest": "^24.0.2",
		"typescript": "^3.5.3"
	},
	"jest": {
		"preset": "react-native"
	},
	"detox": {
		"configurations": {
			"ios.sim.debug": {
				"binaryPath": "ios/build/Catstagram/Build/Products/Debug-iphonesimulator/Catstagram.app",
				"build": "xcodebuild -workspace ios/Catstagram.xcworkspace -scheme Catstagram -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build/Catstagram",
				"type": "ios.simulator",
				"name": "iPhone 7"
			},
			"android.emu.debug": {
				"binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
				"build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
				"type": "android.emulator",
				"name": "Nexus_5_API_27"
			},
			"android.emu.release": {
				"binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
				"build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
				"type": "android.emulator",
				"name": "Nexus_5_API_27"
			}
		},
		"test-runner": "jest"
	}
}
